{
  "name": "hacknews",
  "version": "1.0.0",
  "description": "A Hacker News clone with Next.js",
  "main": "server.js",
  "scripts": {
    "format": "prettier \"**/*.+(js|json|less|css|ts|tsx|md)\" --write",
    "dev": "next",
    "start": "next start",
    "build": "next build"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/seb7887/hacknews.git"
  },
  "author": "Sebastian Matias Segura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/seb7887/hacknews/issues"
  },
  "homepage": "https://github.com/seb7887/hacknews#readme",
  "dependencies": {
    "isomorphic-unfetch": "^3.0.0",
    "next": "^7.0.2",
    "react": "^16.7.0",
    "react-dom": "^16.7.0",
    "styled-components": "^4.1.3"
  },
  "devDependencies": {
    "babel-plugin-styled-components": "^1.10.0",
    "prettier": "^1.16.1"
  },
  "babel": {
    "presets": [
      "next/babel"
    ],
    "plugins": [
      [
        "styled-components",
        {
          "ssr": true,
          "displayName": true,
          "preprocess": false
        }
      ]
    ]
  }
}
